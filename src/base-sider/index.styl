.page-sider
  position fixed
  top 64px
  left 0
  width 200px
  z-index 3
  height calc(100vh - 64px)
  background rgb(179, 196, 255)
  overflow hidden 
  overflow-y auto
  padding 16px 0
  .ant-menu-inline, .ant-menu-item-selected
    border-right none
    background transparent !important
    // color white
  .ant-menu-inline
    .ant-menu-item-selected > a
      color white 